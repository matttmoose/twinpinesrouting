{
  "routes": [
    {
      "route": "/",
      "serve": "/index.html"
    },
    {
      "route": "/favicon.ico",
      "serve": "/favicon.ico"
    },
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/:path",
      "rewrite": "/api/redirect"
    }
  ]
}
